// C# code goes here
using System;
using System.IO;

class Program
{
    static void Main()
    {
        CreateDirectories("templates", "static", "uploads");
    }

    static void CreateDirectories(params string[] directories)
    {
        foreach (var dir in directories)
        {
            if (!Directory.Exists(dir))
            {
                Directory.CreateDirectory(dir);
                Console.WriteLine($"Created directory: {dir}");
            }
            else
            {
                Console.WriteLine($"Directory already exists: {dir}");
            }
        }
    }
}